import java.util.PriorityQueue;
import java.util.Comparator;

/*
 * Stack → LIFO (Last In First Out)
 * Queue → FIFO (First In First Out)
 * Implemented using PriorityQueue with custom comparator
 */
public class PriorityQueueStackQueue {

    public static void main(String[] args) {

        // Stack using PriorityQueue
        PriorityQueue<Integer> stack =
                new PriorityQueue<>(Comparator.reverseOrder());

        stack.add(1);
        stack.add(2);
        stack.add(3);

        System.out.println("Stack Pop: " + stack.poll());

        // Queue using PriorityQueue
        PriorityQueue<Integer> queue = new PriorityQueue<>();

        queue.add(1);
        queue.add(2);
        queue.add(3);

        System.out.println("Queue Remove: " + queue.poll());
    }
}
